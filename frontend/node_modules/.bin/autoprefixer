#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/media/morpheus1213/rll/projects/daerp-laravel/frontend/node_modules/autoprefixer/bin/node_modules:/media/morpheus1213/rll/projects/daerp-laravel/frontend/node_modules/autoprefixer/node_modules:/media/morpheus1213/rll/projects/daerp-laravel/frontend/node_modules:/media/morpheus1213/rll/projects/daerp-laravel/node_modules:/media/morpheus1213/rll/projects/node_modules:/media/morpheus1213/rll/node_modules:/media/morpheus1213/node_modules:/media/node_modules:/node_modules:/media/morpheus1213/rll/projects/daerp-laravel/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/media/morpheus1213/rll/projects/daerp-laravel/frontend/node_modules/autoprefixer/bin/node_modules:/media/morpheus1213/rll/projects/daerp-laravel/frontend/node_modules/autoprefixer/node_modules:/media/morpheus1213/rll/projects/daerp-laravel/frontend/node_modules:/media/morpheus1213/rll/projects/daerp-laravel/node_modules:/media/morpheus1213/rll/projects/node_modules:/media/morpheus1213/rll/node_modules:/media/morpheus1213/node_modules:/media/node_modules:/node_modules:/media/morpheus1213/rll/projects/daerp-laravel/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../autoprefixer/bin/autoprefixer" "$@"
else
  exec node  "$basedir/../autoprefixer/bin/autoprefixer" "$@"
fi
