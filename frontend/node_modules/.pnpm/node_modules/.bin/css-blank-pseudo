#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/media/morpheus1213/rll/projects/daerp-laravel/frontend/node_modules/.pnpm/css-blank-pseudo@3.0.3_postcss@8.4.47/node_modules/css-blank-pseudo/dist/node_modules:/media/morpheus1213/rll/projects/daerp-laravel/frontend/node_modules/.pnpm/css-blank-pseudo@3.0.3_postcss@8.4.47/node_modules/css-blank-pseudo/node_modules:/media/morpheus1213/rll/projects/daerp-laravel/frontend/node_modules/.pnpm/css-blank-pseudo@3.0.3_postcss@8.4.47/node_modules:/media/morpheus1213/rll/projects/daerp-laravel/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/media/morpheus1213/rll/projects/daerp-laravel/frontend/node_modules/.pnpm/css-blank-pseudo@3.0.3_postcss@8.4.47/node_modules/css-blank-pseudo/dist/node_modules:/media/morpheus1213/rll/projects/daerp-laravel/frontend/node_modules/.pnpm/css-blank-pseudo@3.0.3_postcss@8.4.47/node_modules/css-blank-pseudo/node_modules:/media/morpheus1213/rll/projects/daerp-laravel/frontend/node_modules/.pnpm/css-blank-pseudo@3.0.3_postcss@8.4.47/node_modules:/media/morpheus1213/rll/projects/daerp-laravel/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../css-blank-pseudo/dist/cli.cjs" "$@"
else
  exec node  "$basedir/../css-blank-pseudo/dist/cli.cjs" "$@"
fi
